<network ipv6='yes'>
  <name>192_9_2</name>
  <bridge name='swan92' stp='on' delay='0' />
  <mac address='12:00:00:32:32:92'/>
  <ip address='192.9.2.253' netmask='255.255.255.0'>
    <dhcp>
      <range start='192.9.2.128' end='192.9.2.196' />
    </dhcp>
  </ip>
  <ip family="ipv6" address="2001:db8:9:2::253" prefix="64"/>
</network>
